# Source definitions
if [[ -f ~/.bashrc ]]; then
    . ~/.bashrc
elif [[ -f /etc/bashrc ]]; then
    . /etc/bashrc
fi

# Start X
if [[ "$(tty)" = "/dev/tty1" ]]; then
    startx
elif [[ "$(tty)" = "/dev/tty2" ]]; then
    qtile start -b wayland
elif [[ "$(tty)" = "/dev/tty7" ]]; then
    start plasma-wayland
fi
